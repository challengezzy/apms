				var content:String = item["CONTENT"];
				var acnum:String = item["ACNUM"];
				var type:String = item["TYPE"];
				var typestr:String = "告警";
				if(type == "F"){
					typestr = "故障";
				}
				tip += typestr + "\n" + "机号："+acnum+" 章节："+atano;
				tip += "\n"+ DateUtil.dateToString(item[xField],1);
				tip += "\n"+content;
				
				return tip;
			}
