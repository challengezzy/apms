			public function trendTipStrFunc(e:HitData,unitStr:String=""):String {
				var item:Object = e.item;
				
				var yField:String = "";
				var xField:String = "";
				var seriesName:String = "值";
				if(e.element is LineSeries){
					var ls:LineSeries = e.element as LineSeries;
					yField = ls.yField;
					xField = ls.xField;
					seriesName = ls.displayName;
				}else if(e.element is PlotSeries){
					var ps:PlotSeries = e.element as PlotSeries;
					yField = ps.yField;
					xField = ps.xField;
					seriesName = ps.displayName;
				}
				var tip:String = seriesName;
				var position:String;
				var number:String;
				if(e.contextColor==0x0000ff || e.contextColor==0x8FE870){
					position = "1#";
					number = item["ESN_1"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}else if(e.contextColor==0xff0000 || e.contextColor==0x81E55E){
					position = "2#";
					number = item["ESN_2"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}
				if(item[xField] is Date){
					tip += "\n"+ DateUtil.dateToString(item[xField],1);
				}else if(item[xField] is Number){
				    var dateTemp:Date = DateUtil.parseMsToDate(item[xField]);
					tip += "\n"+ DateUtil.dateToString(dateTemp,1);
				}
				
				tip += "\n"+ item[yField]+unitStr;
				
				return tip;
			}
			public function trendTipStrFuncRate(e:HitData,unitStr:String=""):String {
				var item:Object = e.item;
				
				var yField:String = "";
				var xField:String = "";
				var seriesName:String = "值";
				if(e.element is LineSeries){
					var ls:LineSeries = e.element as LineSeries;
					xField = item[ls.xField];
					
					yField = ls.yField;
					seriesName = ls.displayName;
				}else if(e.element is PlotSeries){
					var ps:PlotSeries = e.element as PlotSeries;
					xField = DateUtil.dateToString(item[ps.xField],1);
					
					yField = ps.yField;
					seriesName = ps.displayName;
				}
				var tip:String = seriesName;
				var position:String;
				var number:String;
				if(e.contextColor==0x0000ff || e.contextColor==0x8FE870){
					position = "1#";
					number = item["ESN_1"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}else if(e.contextColor==0xff0000 || e.contextColor==0x81E55E){
					position = "2#";
					number = item["ESN_2"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}
				if(item[xField] is Date){
					tip += "\n"+ DateUtil.dateToString(item[xField],1);
				}else if(item[xField] is Number){
					var dateTemp:Date = DateUtil.parseMsToDate(item[xField]);
					tip += "\n"+ DateUtil.dateToString(dateTemp,1);
				}else{
					tip += "\n"+ xField;
				}
				tip += "\n"+ item[yField]+unitStr;
				
				return tip;
			} 
			public function trendTipStrFunc_D(e:HitData,unitStr:String=""):String {
				var item:Object = e.item;
				
				var yField:String = "";
				var xField:String = "";
				var seriesName:String = "值";
				if(e.element is LineSeries){
					var ls:LineSeries = e.element as LineSeries;
					yField = ls.yField;
					xField = ls.xField;
					seriesName = ls.displayName;
				}else if(e.element is PlotSeries){
					var ps:PlotSeries = e.element as PlotSeries;
					yField = ps.yField;
					xField = ps.xField;
					seriesName = ps.displayName;
				}
				var tip:String = seriesName;
				var position:String;
				var number:String;
				if(e.contextColor==0x0000ff || e.contextColor==0x8FE870){
					position = "1#";
					number = item["ESN_1"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}else if(e.contextColor==0xff0000 || e.contextColor==0x81E55E){
					position = "2#";
					number = item["ESN_2"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}
				if(item[xField] is Date){
					tip += "\n"+ DateUtil.dateToString(item[xField],1);
				}else if(item[xField] is Number){
					var dateTemp:Date = DateUtil.parseMsToDate(item[xField]);
					tip += "\n"+ DateUtil.dateToString(dateTemp,1);
				}
				tip += "\n"+"原值："+ item[yField.substring(8,yField.length)]
				tip += "\n"+"偏离度：" + item[yField]+unitStr;
				
				return tip;
			}
			public function trendTipStrFunc_x(e:HitData,unitStr:String):String {
				var item:Object = e.item;
				
				var yField:String = "";
				var xField:String = "";
				var seriesName:String = "值";
				if(e.element is LineSeries){
					var ls:LineSeries = e.element as LineSeries;
					yField = ls.yField;
					xField = ls.xField;
					seriesName = ls.displayName;
				}else if(e.element is PlotSeries){
					var ps:PlotSeries = e.element as PlotSeries;
					yField = ps.yField;
					xField = ps.xField;
					seriesName = ps.displayName;
				}
				var tip:String = seriesName;
				var position:String;
				var number:String;
				if(e.contextColor==0x0000ff || e.contextColor==0x8FE870){
					position = "1#";
					number = item["ESN_1"];
					tip += "\n"+"位置："+position+" 序号："+number;

				}else if(e.contextColor==0xff0000 || e.contextColor==0x81E55E){
					position = "2#";
					number = item["ESN_2"];
					tip += "\n"+"位置："+position+" 序号："+number;
				}
				if(item[xField] is Date){
					tip += "\n"+ DateUtil.dateToString(item[xField],1);
				}else if(item[xField] is Number){
					var dateTemp:Date = DateUtil.parseMsToDate(item[xField]);
					tip += "\n"+ DateUtil.dateToString(dateTemp,1);
				}else {
					tip += "\n"+ DateUtil.dateToString(item["UTCDATE"],1);
					tip += "\n"+ item[xField];
				}
				
				tip += "\n"+ item[yField]+unitStr;
				return tip;
			}
			
