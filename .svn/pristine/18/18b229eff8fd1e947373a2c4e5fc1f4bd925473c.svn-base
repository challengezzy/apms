				//远程服务添加监听
				engineService.getIaevEngineSildeOilChartData.addEventListener(ResultEvent.RESULT,getIaevEngineSildeOilChartDataHandler);
				engineService.getIaevEngineSildeOilChartData.addEventListener(FaultEvent.FAULT,getIaevEngineSildeOilChartDataErrorHandler);	
			}
			//Flex页面构建完处理
